{
  "name": "alex mbugua's website",
  "type": "module",
  "version": "0.0.1",
  "homepage": "https://asce1062.github.io/asce1062",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "format": "prettier --write . --ignore-path .prettierignore",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "predeploy": "astro check && COMMIT_REF=$(git rev-parse HEAD) astro build",
    "deploy": "gh-pages -d dist --branch alexmbugua --nojekyll --cname alexmbugua.me --dotfiles",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:netlify:preview": "netlify deploy",
    "preview": "astro preview",
    "preview:netlify": "netlify dev",
    "astro": "astro",
    "check": "astro check",
    "knip": "knip",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/rss": "^4.0.15",
    "@astrojs/sitemap": "^3.7.0",
    "@pagefind/default-ui": "^1.4.0",
    "@rehype-pretty/transformers": "^0.13.2",
    "@stefanprobst/remark-extract-toc": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@vite-pwa/astro": "^1.2.0",
    "astro": "^5.16.16",
    "astro-og-canvas": "^0.10.0",
    "astro-pagefind": "^1.8.5",
    "astro-themes": "^0.2.7",
    "autoprefixer": "^10.4.23",
    "lightningcss": "^1.31.1",
    "pagefind": "^1.4.0",
    "postcss": "^8.5.6",
    "postcss-nesting": "^14.0.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-pretty-code": "^0.14.1",
    "remark-slug": "^7.0.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "knip": "^5.82.1",
    "lint-staged": "^16.2.7",
    "netlify-cli": "^23.14.0",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-toml": "^2.0.6"
  },
  "lint-staged": {
    "*.{js,ts,mjs,astro}": [
      "eslint --fix",
      "prettier --write --ignore-path .prettierignore"
    ],
    "*.{json,md,mdx,css,yml,yaml,toml}": [
      "prettier --write --ignore-path .prettierignore"
    ]
  }
}
