{
  "name": "alex mbugua's website",
  "type": "module",
  "version": "0.0.1",
  "homepage": "https://asce1062.github.io/asce1062",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "format": "prettier --write . --ignore-path .prettierignore",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "predeploy": "astro check && astro build",
    "deploy": "gh-pages -d dist --branch alexmbugua --nojekyll --cname alexmbugua.me --dotfiles",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "knip": "knip",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.6",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^6.0.2",
    "@pagefind/default-ui": "^1.4.0",
    "@rehype-pretty/transformers": "^0.13.2",
    "@stefanprobst/remark-extract-toc": "^2.2.0",
    "astro": "^5.14.1",
    "astro-og-canvas": "^0.7.0",
    "astro-pagefind": "^1.8.5",
    "gh-pages": "^6.3.0",
    "pagefind": "^1.4.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-pretty-code": "^0.14.1",
    "remark-slug": "^7.0.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "astro-eslint-parser": "^1.2.2",
    "astro-themes": "^0.2.6",
    "eslint": "^9.37.0",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "knip": "^5.64.1",
    "lightningcss": "^1.30.2",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,ts,mjs,astro}": [
      "eslint --fix",
      "prettier --write --ignore-path .prettierignore"
    ],
    "*.{json,md,mdx,css,yml,yaml}": [
      "prettier --write --ignore-path .prettierignore"
    ]
  }
}
